 <import src="../stars/stars-template.wxml" /> 
<template name="music-item-template">
    <view class="music-item" data-id="{{id}}" ontap="listDetail">
        <image src="{{coverageUrl}}" class="cover"></image>
        <view class="music-item-name">
            <text>{{name}}</text>
        </view>
        <template is="stars-template" data="{{stars:stars,score:score}}"></template>
    </view>
</template>